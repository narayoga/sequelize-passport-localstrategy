<html lang="en">
<%- include("component/head")  %> 
<style>
  .container{
    width: fit-content;
    padding: 25px 0;
    margin: 0 auto;
  }
</style>
<body>
  <div class="container">
    <form method="post" action="/login">
        <div class="mb-3">
          <label for="email" class="form-label ms-2">username</label>
          <input class="form-control" id="username" name="username" type="text" required></input>
        </div>
        <div class="mb-3">
          <label for="password" class="form-label ms-2">password</label>
          <input class="form-control" id="password" name="password" type="password" required></input>
        </div>
        <div class="mb-3" >
            <button type="submit" class="submit btn-primary form-control mx-auto">submit</button>
        </div>
      </form>
  </div> 
</body>
</html>