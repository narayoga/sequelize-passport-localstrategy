<html lang="en">
<%- include("component/head")  %> 
<style>
  .card{
    margin-top: 50px;
  }
  .navbar-brand img{
  width: 100px;
  }
</style>
<body> 
  <%- include("component/navbar")%> 
  <div class="container">
    <h2>GUILD LIST</h2>

    <% for( let i = 1; i < list.length; i++ ) { %>
      <div class="card" style="width: 20rem;">
        <div class="title" style="width: 100%;">
          <div class="inner-title mx-auto" style="width: 60%;">
            <h2 class="text-center"><%= list[i].name %></h2>
          </div>
        </div>
        <div class="card-body">
          <p class="card-text">win: <span class="fw-bold"><%= list[i].win %></span> </p>
          <p class="card-text">lose: <span class="fw-bold"><%= list[i].lose %></span></p>
          <a href="/admin/guild/<%= list[i].name%>" class="btn btn-primary">detail</a>
        </div>
      </div>
    <% } %>

  </div>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>
</html>