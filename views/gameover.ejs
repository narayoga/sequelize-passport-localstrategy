<html lang="en">
<%- include("component/head")  %> 
<style>
  .dropdown-menu{
    width: 25em !important;
  }
  .navbar-brand img{
    width: 100px;
  }
</style>
<body>
  <% let id = 1  %> 
  <div class="container" style="margin-top: 20em;">
    <h2 class="text-center" style="margin-bottom: 50px;">Rank List</h2>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">no</th>
          <th scope="col">username</th>
          <th scope="col">win</th>
          <th scope="col">lose</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(e => { %>
        <tr>
          <th scope="row"><%= id++ %> </th>
          <td><%= e.biodata.username %></td>
          <td><%= e.history.win %></td>
          <td><%= e.history.lose %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
    <a class="span" style="float: left;" href="/home">&laquo; HOME</a>
  </div>
  <script src="./list.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
  </body>
</html>

